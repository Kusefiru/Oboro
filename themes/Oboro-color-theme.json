{
	"name": "Oboro",
	"type": "dark",
	"semanticHighlighting": true,
	"semanticTokenColors":{
		"class": "#61c0b2",
		"function": "#f490ca",
		"method": "#f490ca",
		"module": "#62a9d8",
		"type": "#f86c88",
		"parameter": "#e8a0a9",
		"variable": "#e8d4b8",
		"builtinConstant": "#FFBF00",
		// Python tokens
		"selfParameter": "#f86c88"
	},
	"tokenColors": [
		// C Syntax color
		{
			"name": "c.keywords",
			"scope": "keyword.control.c, keyword.control.switch.c, keyword.control.case.c",
			"settings": {
				"foreground": "#ba62fa",
			}
		},
		{
			"name": "c.functions",
			"scope": "entity.name.function.c",
			"settings": {
				"foreground": "#f490ca",
			}
		},
		{
			"name": "c.defines",
			"scope": "entity.name.function.preprocessor.c",
			"settings": {
				"foreground": "#62a9d8",
			}
		},
		{
			"name": "c.types.built-in",
			"scope": "storage.type.built-in.primitive.c, storage.type.built-in.c, support.type.posix-reserved.c, meta.function.definition.parameters.c",
			"settings": {
				"foreground": "#f86c88",
			}
		},
		{
			"name": "c.types.storage",
			"scope": "storage.modifier.c",
			"settings": {
				"foreground": "#f84c70",
			}
		},
		{
			"name": "c.preprocessor",
			"scope": "keyword.control.directive.include.c, keyword.control.directive.define.c, keyword.control.directive.conditional.c",
			"settings": {
				"foreground": "#5e6ad2",
			}
		},
		{
			"name": "c.preprocessor.parameter",
			"scope": "variable.parameter.preprocessor.c, variable.parameter.probably.c",
			"settings": {
				"foreground": "#e8a0a9",
			}
		},
		// Python syntax color
		{
			"name": "python.keywords",
			"scope": "keyword.control.import.python",
			"settings": {
				"foreground": "#5e6ad2",
			}
		},
		{
			"name": "python.keywords",
			"scope": "keyword.control.flow.python",
			"settings": {
				"foreground": "#ba62fa",
				"fontStyle": "italic",
			}
		},
		{
			"name": "python.keywords.storage",
			"scope": "storage.type.class.python, storage.type.function.python, storage.type.function.async.python",
			"settings": {
				"foreground": "#5e6ad2",
			}
		},
		{
			"name": "python.types",
			"scope": "support.type.python",
			"settings": {
				"foreground": "#5e6ad2",
			}
		},
		{
			"name": "python.functions",
			"scope": "entity.name.function.python",
			"settings": {
				"foreground": "#f490ca",
			}
		},
		{
			"name": "python.function-call.parameter",
			"scope": "variable.parameter.function-call.python",
			"settings": {
				"foreground": "#e8a0a9",
			}
		},
		{
			"name": "python.operator",
			"scope": "punctuation.definition.decorator.python",
			"settings": {
				"foreground": "#f84c70",
			}
		},
		// Common colors
		{
			"name": "comments",
			"scope": "comment",
			"settings": {
				"foreground": "#706886",
			}
		},
		{
			"name": "strings",
			"scope": "string.quoted",
			"settings": {
				"foreground": "#aa9dff",
			}
		},
		{
			"name": "numeric",
			"scope": "constant.numeric, constant.language, constant.other",
			"settings": {
				"foreground": "#FFBF00",
			}
		},
		{
			"name": "operator",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#f84c70",
			}
		}
	],
