{
	"name": "Oboro",
	"type": "dark",
	"semanticHighlighting": true,
	"semanticTokenColors":{
		"class": "#61c0b2",
		"function": "#e8aa9b",
		"method": "#e8aa9b",
		"module": "#aa9def",
		"type": "#f06c82",
		"parameter": "#f8e8d4",
		"property": "#f8e8d4",
		"builtinConstant": "#FFBF00",
		"variable": {
			"foreground": "#f8e8d4",
			"fontStyle": "italic"
		},
		"enumMember": {
			"foreground": "#72add8",
			"fontStyle": "italic"
		},
		// Python tokens
		"selfParameter": "#f06c82"
	},
	"tokenColors": [
		// Common colors
		{
			"name": "comments",
			"scope": "comment",
			"settings": {
				"foreground": "#706886",
			}
		},
		{
			"name": "strings",
			"scope": "string.quoted",
			"settings": {
				"foreground": "#aa9def",
			}
		},
		{
			"name": "numeric",
			"scope": "constant.numeric, constant.language, constant.other, constant.character.format",
			"settings": {
				"foreground": "#FFBF00",
			}
		},
		{
			"name": "directives",
			"scope": "keyword.control.directive",
			"settings": {
				"foreground": "#5e6ad2",
			}
		},
		{
			"name": "operator",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#f84c70",
			}
		},
		// Markup languages
		{
			"name": "markup.heading",
			"scope": "markup.heading",
			"settings":{
				"foreground": "#e8aa9b"
			}
		},
		{
			"name": "markup.code",
			"scope": "markup.fenced_code, markup.inline.raw",
			"settings": {
				"foreground": "#aa9def",
			}
		},
		{
			"name": "markup.italic",
			"scope": "markup.italic",
			"settings":{
				"fontStyle": "italic"
			}
		},
		{
			"name": "markup.bold",
			"scope": "markup.bold",
			"settings":{
				"fontStyle": "bold"
			}
		},
		{
			"name": "markup.punctuation",
			"scope": "punctuation.definition.heading, punctuation.definition.list.begin",
			"settings":{
				"foreground": "#f84c70"
			}
		},
		{
			"name": "markup.titles",
			"scope": "entity.name.section.markdown, markup.heading.setext",
			"settings":{
				"foreground": "#e8aa9b"
			}
		},
		{
			"name": "markup.anchors",
			"scope": "entity.name.tag.anchor",
			"settings":{
				"foreground": "#72add8"
			}
		},
		// C Syntax color
		{
			"name": "c.keywords",
			"scope": "keyword.control.c, keyword.control.switch.c, keyword.control.case.c, keyword.control.default.c, keyword.other.typedef.c",
			"settings": {
				"foreground": "#ba72fa",
				"fontStyle": "italic",
			}
		},
		{
			"name": "c.functions",
			"scope": "entity.name.function.c",
			"settings": {
				"foreground": "#e8aa9b",
			}
		},
		{
			"name": "c.defines",
			"scope": "entity.name.function.preprocessor.c",
			"settings": {
				"foreground": "#72add8",
			}
		},
		{
			"name": "c.types.built-in",
			"scope": "storage.type.built-in.primitive.c, storage.type.built-in.c, support.type.posix-reserved.c, meta.function.definition.parameters.c",
			"settings": {
				"foreground": "#f06c82",
			}
		},
		{
			"name": "c.types.storage",
			"scope": "storage.modifier.c, storage.type.struct.c, storage.type.union.c, storage.type.enum.c",
			"settings": {
				"foreground": "#f84c70",
				"fontStyle": "italic",
			}
		},
		{
			"name": "c.preprocessor",
			"scope": "keyword.control.directive.include.c, keyword.control.directive.define.c, keyword.control.directive.conditional.c",
			"settings": {
				"foreground": "#5e6ad2",
				"fontStyle": "italic",
			}
		},
		{
			"name": "c.preprocessor.parameter",
			"scope": "variable.parameter.preprocessor.c, variable.parameter.probably.c",
			"settings": {
				"foreground": "#f8e8d4",
			}
		},
		// Python syntax color
		{
			"name": "python.keywords",
			"scope": "keyword.control.import.python",
			"settings": {
				"foreground": "#5e6ad2",
				"fontStyle": "italic",
			}
		},
		{
			"name": "python.keywords",
			"scope": "keyword.control.flow.python",
			"settings": {
				"foreground": "#ba72fa",
				"fontStyle": "italic",
			}
		},
		{
			"name": "python.keywords.storage",
			"scope": "storage.type.class.python, storage.type.function.python, storage.type.function.async.python",
			"settings": {
				"foreground": "#5e6ad2",
				"fontStyle": "italic",
			}
		},
		{
			"name": "python.types",
			"scope": "support.type.python",
			"settings": {
				"foreground": "#5e6ad2",
			}
		},
		{
			"name": "python.functions",
			"scope": "entity.name.function.python",
			"settings": {
				"foreground": "#e8aa9b",
			}
		},
		{
			"name": "python.function-call.parameter",
			"scope": "variable.parameter.function-call.python",
			"settings": {
				"foreground": "#f8e8d4",
			}
		},
		{
			"name": "python.operator",
			"scope": "punctuation.definition.decorator.python, meta.fstring.python, storage.type.string.python",
			"settings": {
				"foreground": "#f84c70",
			}
		},
		// Makefile syntax colors
		{
			"name": "makefile.target",
			"scope": "entity.name.function.target.makefile",
			"settings": {
				"foreground": "#e8aa9b",
			}
		},
		{
			"name": "makefile.variable",
			"scope": "variable.other.makefile",
			"settings": {
				"foreground": "#f8e8d4",
			}
		},
		{
			"name": "makefile.punctuation",
			"scope": "punctuation.definition.variable.makefile, punctuation.separator.key-value.makefile, constant.character.escape.continuation.makefile",
			"settings": {
				"foreground": "#f84c70",
			}
		},
		{
			"name": "makefile.phonu",
			"scope": "support.function.target.PHONY.makefile",
			"settings": {
				"foreground": "#f06c82",
			}
		},
		{
			"name": "makefile.functions",
			"scope": "meta.scope.function-call.makefile, variable.language.makefile",
			"settings": {
				"foreground": "#5e6ad2",
			}
		},
		{
			"name": "makefile.specia-variables",
			"scope": "variable.language.makefile",
			"settings": {
				"foreground": "#72add8",
			}
		},
		// Batch syntax colors
		{
			"name": "bat.functions",
			"scope": "keyword.command.batchfile",
			"settings": {
				"foreground": "#5e6ad2",
			}
		},
		{
			"name": "bat.keywords",
			"scope": "keyword.control.conditional.batchfile, keyword.control.statement.batchfile",
			"settings": {
				"foreground": "#ba72fa",
			}
		},
		{
			"name": "bat.punctuation",
			"scope": "punctuation.definition.variable.batchfile, punctuation.separator.batchfile, punctuation.definition.variable.begin.batchfile, punctuation.definition.variable.end.batchfile",
			"settings": {
				"foreground": "#f84c70",
			}
		},
		{
			"name": "bat.method",
			"scope": "keyword.other.special-method.batchfile",
			"settings": {
				"foreground": "#e8aa9b",
			}
		},
		{
			"name": "bat.variable",
			"scope": "variable.other.readwrite.batchfile",
			"settings": {
				"foreground": "#f8e8d4",
			}
		},
	],
